<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
"-//Puppy Crawl//DTD Suppressions 1.1//EN"
"http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>
  <suppress checks="MagicNumberCheck"
    files="ToHexCoder|PdfWriter|PdfDocument|ZLibStreamer|PdfWriteHelper|PdfMaker|PdfFactory|UomHelper|PdfContent"/>
  <suppress checks="MagicNumberCheck"
    files="TtfInputStream|ATtfOutputStream|TtfOutputStream|TtfLoader|PdfFontDescriptor|TtfCompactFontMaker|PdfCidFontType2"/>
  <suppress checks="MagicNumberCheck"
    files="TdeMaker|TableMakerHhea|TableMakerMaxp|TableMakerLoca|TableMakerGlyf|TtfHhea|TtfHead|TtfMaxp|TableMakerFc"/>
  <suppress checks="MagicNumberCheck"
    files="StringWriter|TtfBufferInputStream|WriterPdfImage|ImageWriter|TableMakerHmtx"/>
  <!--protected useful for increments e.g. "size++"-->
  <suppress checks="VisibilityModifier"
    files="ATtfOutputStream"/>
</suppressions>
