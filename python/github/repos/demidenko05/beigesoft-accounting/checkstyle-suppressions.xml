    files="UtlPurchaseGoodsServiceLine|UtlSalesGoodsServiceLine"/>
  <suppress checks="MagicNumberCheck"
    files="Manufacture|WarehouseEntry|PurchaseInvoiceLine|SalesInvoiceLine|UsedMaterialLine|AdditionCostLine"/>
  <suppress checks="MagicNumberCheck"
    files="Tax|InvItemType|AccountingEntries|InvItem|DebtorCreditor|InvItemCategory|ManufacturingProcess"/>
  <suppress checks="MagicNumberCheck"
    files="AccSettings|EmployeeCategory|Employee|WorkType|Expense|Property|BankAccount|SrvAccSettings|WageTaxLine|WageLine|Wage"/>
  <suppress checks="MagicNumberCheck"
    files="MngDatabaseAndroid|PaymentTo|PrepaymentTo|PrepaymentFrom|PaymentFrom|BankStatementLine"/>
