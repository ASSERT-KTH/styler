<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.0//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">

<!--表示不执行code style检查-->
<suppressions>

    <!--表示对CodeCheckStyle.java文件的第5行和第7,8,9,10行不执行Indentation检查
    checks的值来至于我们定义的codestyle文件里面的module的name属性值，例如google_checks.xml，custom_check_style.xml
    这里的Indentation表示代码最左边的空格检查
    -->
    <suppress checks="Indentation"
              files="CodeCheckStyle.java"
              lines="5,7-10"/>
    <!--表示对CodeCheckStyle.java文件的第4行不执行JavadocMethod检查-->
    <suppress checks="JavadocMethod"
              files="CodeCheckStyle.java"
              lines="4"/>
</suppressions>