<!DOCTYPE suppressions PUBLIC
     "-//Puppy Crawl//DTD Suppressions 1.0//EN"
     "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">
<suppressions>
  <suppress checks="AvoidInlineConditionals" files=".+\.java" lines="0-9999" />
  <suppress checks="FinalParameters" files=".+\.java" lines="0-9999" />
  <suppress checks="HiddenField" files=".+\.java" lines="0-9999" />
  <suppress checks="JavadocMethod" files=".+Exception\.java" lines="0-9999" />
  <suppress checks="JavadocType" files=".+Exception\.java" lines="0-9999" />
  <suppress checks="JavadocVariable" files=".+Exception\.java" lines="0-9999" />
</suppressions>
